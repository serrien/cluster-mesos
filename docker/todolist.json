{
  "id": "/app/todolist",
  "instances": 1,
  "cpus": 0.2,
  "mem": 32,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "boune/todolist",
       "network": "BRIDGE",
       "privileged": true,
       "portMappings": [
         { "containerPort": 3000, "protocol": "tcp" }
       ],
       "parameters": [
                { "key": "dns", "value": "192.168.33.201" },
                { "key": "dns-search", "value": "service.consul"}
            ]
    }
  },
  "env": {
    "SERVICE_NAME": "todo",
    "SERVICE_TAGS": "production"
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/",
      "gracePeriodSeconds": 5,
      "intervalSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}